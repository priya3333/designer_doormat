# designer_doormat

height,widtht = map(int,input().split()) #taking space seprated input

p = '.|.'  #for making pattern

h = '-'

welcome = 'WELCOME'

for x in range(1,height,2): ###forward loop

    print((p*x).center(width,h))
    
print(welcome.center(width,h))

for x in range(height-1,1,-2): ###reverse loop
    
    print((p*(x-1)).center(width,h))
    
